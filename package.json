    "bcryptjs": "^2.4.3",
    "better-auth": "^1.3.33",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "latest",
    "express-rate-limit": "^7.5.1",
    "gpt-tokenizer": "^3.4.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.2",
    "jsdom": "latest",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
